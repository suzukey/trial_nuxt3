{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["./*"],
      "~/*": ["src/*"],
      "~~/*": ["./*"]
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": [
      "node",
      "nuxt-windicss"
    ]
  }
}
